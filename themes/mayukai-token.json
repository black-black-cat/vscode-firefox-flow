			"settings": {
				"foreground": "#cbccc6b3"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.section"
			],
			"settings": {
				"foreground": "#dadbc0"
			}
		},
		{
			"name": "Punctuation Default",
			"scope": [
				"punctuation.definition"
			],
			"settings": {
				"foreground": "#CBCCC6B3"
			}
		},
		{
			"name": "Accessor",
			"scope": [
				"punctuation.accessor"
			],
			"settings": {
				"foreground": "#f29e74"
			}
		},
		{
			"name": "Types fixes",
			"scope": [
				"source.java storage.type",
				"source.haskell storage.type",
				"source.c storage.type"
			],
			"settings": {
				"foreground": "#9CD1BB"
			}
		},
		{
			"name": "Inherited class type",
