				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#9CD1BB"
			}
		},
		{
			"name": "Lambda arrow",
			"scope": [
				"storage.type.function"
			],
			"settings": {
				"foreground": "#9CD1BB"
			}
		},
		{
			"name": "JavaScript Lambda arrow",
			"scope": [
				"storage.type.function.arrow"
			],
			"settings": {
				"foreground": "#FF6188",
				"fontStyle": "",
			}
		},
		{
			"name": "Java primitive variable types",
			"scope": [
				"source.java storage.type.primitive"
			],
			"settings": {
				"foreground": "#9CD1BB"
			}
