				"support.variable"
			],
			"settings": {
				"foreground": "#FC9867"
			}
		},
		{
			"name": "Storage",
			"scope": [
				"storage"
			],
			"settings": {
				"foreground": "#ffa759"
			}
		},
		{
			"name": "Storage LET CONST",
			"scope": [
				"storage.type"
			],
			"settings": {
				"foreground": "#9CD1BB",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Storage MOFIFIER",
			"scope": [
				"storage.modifier"
			],
			"settings": {
				"foreground": "#FF6188",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Storage ASYNC",
			"scope": [
				"storage.modifier.async"
