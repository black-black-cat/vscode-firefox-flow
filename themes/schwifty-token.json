{
  "$schema": "vscode://schemas/color-theme",
  "name": "Schwifty",
  "type": "dark",
  "tokenColors": [
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "header",
      "settings": {
        "foreground": "#00afc8"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#fb6467",
        "fontStyle": "underline italic"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#fb6467",
        "fontStyle": "underline italic"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#bca178"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#bca178",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#00afc8"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Brackets, Braces, & Other Punctuation",
      "scope": [
        "keyword.operator.dereference.java",
        "keyword.operator.navigation.groovy",
        "keyword.operator.other.powershell",
        "keyword.other.statement-separator.powershell",
        "meta.brace.round",
        "meta.function-call punctuation",
        "string.other.math.shell punctuation.definition.string.begin",
        "string.other.math.shell punctuation.definition.string.end",
        "string.other.math.shell",
        "punctuation.definition.arguments.begin",
        "punctuation.definition.arguments.end",
        "punctuation.definition.attribute-selector.end.bracket.square",
        "punctuation.definition.attribute-selector.begin.bracket.square",
        "punctuation.definition.entity.begin",
        "punctuation.definition.entity.end",
        "punctuation.definition.string.begin.yaml",
        "punctuation.definition.string.end.yaml",
        "punctuation.definition.type.begin",
        "punctuation.definition.type.end",
        "punctuation.section.scope.begin",
        "punctuation.section.scope.end",
        "punctuation.separator.list.comma.css",
        "source.yaml string",
        "string.template meta.brace",
        "string.template punctuation.accessor"
      ],
      "settings": {
        "foreground": "#fdd9a2"
      }
    },
    {
      "name": "Brackets, Braces, & Other Punctuation (Edge Cases)",
      "scope": [
        "constant.language.empty-list.haskell",
        "markup.fenced_code.block.markdown punctuation",
        "punctuation.definition.sequence.begin.yaml",
        "punctuation.definition.sequence.end.yaml",
        "storage.modifier.array.bracket.square.objc"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#fdd9a2"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring.multi",
        "unused.comment",
        "wildcard.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#008ca0"
      }
    },
    {
      "name": "Comments (Buffered)",
      "scope": "string.comment.buffered.block.pug",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#917c5d"
      }
    },
    {
      "name": "Comments (Punctuation)",
      "scope": [
        "punctuation.definition.comment",
        "string.quoted.docstring.multi.python punctuation.definition.string.begin",
        "string.quoted.docstring.multi.python punctuation.definition.string.end",
