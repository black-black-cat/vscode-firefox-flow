			"settings": {
				"foreground": "#9CD1BB",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword"
			],
			"settings": {
				"foreground": "#ffa759"
			}
		},
		{
			"name": "Keyword Control",
			"scope": [
				"keyword.control"
			],
			"settings": {
				"foreground": "#FF6188"
			}
		},
